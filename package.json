{
  "name": "docman",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "webpack -w",
    "start": "npm-run-all --parallel serve",
    "test": "NODE_ENV=test mocha",
    "serve": "babel-watch app.js",
    "usermodels": "sequelize model:create --name Users --attributes 'username:string, firstname:string, lastname:string, roles:integer'",
    "documentmodels": "sequelize model:create --name Documents --attributes 'title:string, creator:string, excerpt:string, access:integer'",
    "migrate": "npm run usermodels && npm run documentmodels"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "4.14.0",
    "express-session": "^1.15.0",
    "jquery": "^3.1.1",
    "js-logger": "^1.3.0",
    "material-ui": "^0.16.7",
    "morgan": "~1.7.0",
    "muicss": "^0.9.9-rc2",
    "mysql": "^2.13.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "~2.0.0-beta6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "sequelize": "^3.30.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.1.1",
