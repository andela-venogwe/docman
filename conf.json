{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./"],
        "includePattern": ".+\\.js$",
        "excludePattern": "(docs|node_modules|coverage|config)"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "systemName": "Document Management System",
        "copyright": "Enogwe Victor &copy; 2017",
        "navType": "vertical",
        "theme": "flatly",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": false,
        "search": true
    },
    "opts": {
        "template": "./node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "docs",
        "recurse": true,
        "private": true
    }
}